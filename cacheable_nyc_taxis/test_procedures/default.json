{
  "name": "cacheable-queries",
  "description": "Indexes the whole document corpus using a setup that will lead to a larger indexing throughput than the default settings and produce a smaller index (higher compression rate). Document ids are unique so all index operations are append only. After that a couple of queries are run.",
  "default": true,
  "schedule": [
    {
      "parallel": {
        "tasks": [
          {
            "name":"cheap-tip-amount",
            "operation":"cheap-tip-amount",
            "warmup-iterations": 1200,
            "iterations": 120000,
            "target-throughput": 6000
          },
          {
            "name":"cheap-fare-amount",
            "operation":"cheap-fare-amount",
            "warmup-iterations": 1200,
            "iterations": 120000,
            "target-throughput": 6000
          },
          {
            "name":"cheap-total-amount",
            "operation":"cheap-total-amount",
            "warmup-iterations": 1200,
            "iterations": 120000,
            "target-throughput": 6000
          },
          {
            "name":"cheap-pickup",
            "operation":"cheap-pickup",
            "warmup-iterations": 1200,
            "iterations": 120000,
            "target-throughput": 6000
          },
          {
            "name":"cheap-dropoff",
            "operation":"cheap-dropoff",
            "warmup-iterations": 1200,
            "iterations": 120000,
            "target-throughput": 6000
          },
          {
            "name":"autohisto-agg",
            "operation":"autohisto_agg",
            "warmup-iterations": 300,
            "iterations": 40000,
            "target-throughput": 1500
          },
          {
            "name":"date-histogram-agg",
            "operation":"date_histogram_agg",
            "warmup-iterations": 300,
            "iterations": 2500,
            "target-throughput": 1500
          },
          {
            "name":"date-histogram-calendar-interval",
            "operation":"date_histogram_calendar_interval",
            "warmup-iterations": 300,
            "iterations": 40000,
            "target-throughput": 1500
          },
          {
            "name":"date-histogram-fixed-interval",
            "operation":"date_histogram_fixed_interval",
            "warmup-iterations": 300,
            "iterations": 40000,
            "target-throughput": 1500
          },
          {
            "name":"auto-date-histogram",
            "operation":"auto_date_histogram",
            "warmup-iterations": 300,
            "iterations": 40000,
            "target-throughput": 1500
          }
        ]
      }
    }
  ]
}