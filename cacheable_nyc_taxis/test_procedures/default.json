    {
      "name": "cacheable-queries",
      "description": "Indexes the whole document corpus using a setup that will lead to a larger indexing throughput than the default settings and produce a smaller index (higher compression rate). Document ids are unique so all index operations are append only. After that a couple of queries are run.",
      "default": true,
      "schedule": [
        {
          "parallel": {
            "tasks": [
              {
                "name": "query-1",
                "operation": {
                  "operation-type": "search",
                  "body": {
                    "size": 0,
                    "query": {
                      "range": {
                        "pickup_datetime": {
                          "gte": "2000-01-01 00:00:00",
                          "lte": "2023-12-31 23:59:59"
                        }
                      }
                    },
                    "aggs": {
                      "vendor_id_terms": {
                        "terms": {
                          "field": "vendor_id",
                          "size": 10000
                        },
                        "aggs": {
                          "trip_type_terms": {
                            "terms": {
                              "field": "trip_type",
                              "size": 10000
                            },
                            "aggs": {
                              "payment_type_terms": {
                                "terms": {
                                  "field": "payment_type",
                                  "size": 10000
                                },
                                "aggs": {
                                  "avg_fare_amount": {
                                    "avg": {
                                      "field": "fare_amount"
                                    }
                                  },
                                  "sum_total_amount": {
                                    "sum": {
                                      "field": "total_amount"
                                    }
                                  },
                                  "min_fare_amount": {
                                    "min": {
                                      "field": "fare_amount"
                                    }
                                  },
                                  "max_fare_amount": {
                                    "max": {
                                      "field": "fare_amount"
                                    }
                                  },
                                  "cardinality_trip_id": {
                                    "cardinality": {
                                      "field": "trip_id"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                },
                "settings": {
                  "index.requests.cache.enable": true
                },
                "warmup-iterations": 0,
                "iterations": 1,
                "target-throughput": 100
              },
              {
                "name": "query-2",
                "operation": {
                  "operation-type": "search",
                  "body": {
                    "size": 0,
                    "query": {
