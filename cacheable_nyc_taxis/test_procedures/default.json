      "schedule": [
        {
          "parallel": {
            "tasks": [
              {
                "name":"cheap-tip-amount",
                "operation":"cheap-tip-amount",
                "settings": {
                  "index.requests.cache.enable":true
                },
                "warmup-iterations": 0,
                "iterations": 1,
                "target-throughput": 100
              },
              {
                "name":"cheap-fare-amount",
